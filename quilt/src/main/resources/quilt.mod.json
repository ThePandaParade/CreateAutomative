{
  "schema_version": 1,
  "mixin": [
    "create_automative.mixins.json",
    "create_automative-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "create_automative",
    "version": "${version}",
    "metadata": {
      "name": "Create: Automative",
      "description": "Automate everything with Create!",
      "contributors": {
        "Robyn-Dawn 'ThePandaParade' B. <robyn@pandapa.ws>": "Lead Developer"
      },
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "icon": "assets/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.create_automative.quilt.CreateAutomativeQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.1.12"
      },
      {
        "id": "create",
        "version": ">=0.5.0"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}